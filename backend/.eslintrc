{
  "env": {
    "commonjs": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "consistent": false
        },
        "ObjectPattern": {
          "multiline": true,
          "consistent": false
        },
        "ImportDeclaration": {
          "multiline": true
        },
        "ExportDeclaration": {
          "multiline": true,
          "minProperties": 2
        }
      }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2
      }
    ]
  }
}